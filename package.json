{
  "name": "runbookpilot",
  "version": "0.1.0",
  "description": "AI-assisted SOC runbook automation with graduated autonomy (L0/L1/L2)",
  "module": "src/cli/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "cli": "bun run src/cli/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "build": "bun build src/cli/index.ts --outdir dist --target bun"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "latest",
    "@types/uuid": "^11.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "soc",
    "runbook",
    "automation",
    "incident-response",
    "security",
    "soar"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sim-Security/RunbookPilot.git"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "commander": "^14.0.3",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "uuid": "^13.0.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  }
}
